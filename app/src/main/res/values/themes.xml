<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ViewResources" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/mariner</item>
        <item name="colorPrimaryVariant">@color/mariner</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/shamrock</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/outer_space</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="iconColor">@color/dusty_gray</item>
        <item name="errorColor">@color/cinnabar</item>
        <item name="backgroundTint">@color/gallery</item>
        <item name="textButtonColor">@color/white</item>
        <item name="subTitleColor">@color/outer_space</item>
        <item name="subLabelTextColor">@color/mariner</item>
        <item name="titleColor">@color/outer_space</item>
        <item name="buttonIconColor">@color/dusty_gray</item>
        <item name="labelTextColor">@color/mariner</item>
    </style>


</resources>